<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingDef ParentName="ApparelMakeableBase" Name="CASFlakBootsBase" Abstract="true">
    <defName>CASFlakBoots</defName>
    <label>tactical boots</label>
    <description>A pair of sturdy leather boots.</description>
    <techLevel>Industrial</techLevel>
    <tradeability>Sellable</tradeability>
    <generateCommonality>0.1</generateCommonality>
    <costStuffCount>30</costStuffCount>
    <stuffCategories>
      <li>Leathery</li>
    </stuffCategories>
    <costList>
      <Steel>4</Steel>
    </costList>
    <statBases>
      <MaxHitPoints>100</MaxHitPoints>
      <WorkToMake>2000</WorkToMake>
      <Mass>2.0</Mass>
      <StuffEffectMultiplierArmor>0.25</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Cold>0.4</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>0.10</StuffEffectMultiplierInsulation_Heat>
      <EquipDelay>2.0</EquipDelay>
    </statBases>
    <equippedStatOffsets>
      <MoveSpeed>0.1</MoveSpeed>
    </equippedStatOffsets>
    <thingCategories Inherit="False">
      <li>CASFootWear</li>
    </thingCategories>
    <apparel>
      <bodyPartGroups>
        <li>Feet</li>
      </bodyPartGroups>
      <layers>
        <li>Middle</li>
      </layers>
      <tags>
        <li>IndustrialBasic</li>
      </tags>
      <defaultOutfitTags>
        <li>Soldier</li>
      </defaultOutfitTags>
    </apparel>
    <recipeMaker>
      <unfinishedThingDef>UnfinishedArmor</unfinishedThingDef>
      <researchPrerequisite>ComplexClothing</researchPrerequisite>
      <skillRequirements>
        <Crafting>3</Crafting>
      </skillRequirements>
      <recipeUsers Inherit="False">
        <li>ElectricTailoringBench</li>
        <li>HandTailoringBench</li>
      </recipeUsers>
    </recipeMaker>
  </ThingDef>
  <ThingDef ParentName="CASFlakBootsBase">
    <defName>CASFlakBootsArctic</defName>
    <label>tactical boots (Arctic)</label>
    <description>A pair of sturdy leather boots with steel re-inforcements designed for being worn in an arctic theatre.</description>
    <graphicData>
      <texPath>Things/Item/CASFootwear/CASFlakBootsArctic</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <color>(250,250,250)</color>
    </graphicData>
    <apparel>
      <tags>
