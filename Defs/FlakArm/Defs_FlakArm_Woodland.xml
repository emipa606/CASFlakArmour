<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <ThingDef ParentName="ArmorHelmetMakeableBase">
    <defName>WoodlandAdvancedHelmet</defName>
    <label>flak helmet (woodland)</label>
    <description>A lightweight combat helmet with a complex design, reinforced with plasteel. Not effective against blunt weapons. This helmet has a woodland camo design.</description>
    <techLevel>Industrial</techLevel>
    <recipeMaker>
      <researchPrerequisite>FlakArmor</researchPrerequisite>
      <unfinishedThingDef>UnfinishedMetallicTechArmor</unfinishedThingDef>
      <skillRequirements>
        <Crafting>5</Crafting>
      </skillRequirements>
    </recipeMaker>
    <graphicData>
      <texPath>Things/Item/FlakArm/Woodland/Apparel/WoodlandAdvancedHelmet</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <statBases>
      <WorkToMake>8200</WorkToMake>
      <MaxHitPoints>120</MaxHitPoints>
      <Mass>1.2</Mass>
      <StuffEffectMultiplierArmor>0.7</StuffEffectMultiplierArmor>
      <StuffEffectMultiplierInsulation_Cold>0.15</StuffEffectMultiplierInsulation_Cold>
      <StuffEffectMultiplierInsulation_Heat>0</StuffEffectMultiplierInsulation_Heat>
      <EquipDelay>2.5</EquipDelay>
    </statBases>
    <stuffCategories>
      <li>Metallic</li>
    </stuffCategories>
    <costStuffCount>40</costStuffCount>
    <costList>
      <ComponentIndustrial>2</ComponentIndustrial>
      <Plasteel>10</Plasteel>
    </costList>
    <apparel>
      <bodyPartGroups>
        <li>UpperHead</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Item/FlakArm/Woodland/Head/WoodlandAdvancedHelmet</wornGraphicPath>
      <layers>
        <li>Overhead</li>
      </layers>
      <tags>
        <li>PassiveCamo_Woodland_High</li>
        <li>IndustrialMilitaryAdvanced</li>
      </tags>
      <defaultOutfitTags>
        <li>Soldier</li>
      </defaultOutfitTags>
    </apparel>
    <thingCategories Inherit="false">
      <li>CASHeadgear</li>
    </thingCategories>
  </ThingDef>
  <ThingDef ParentName="ArmorMachineableBase">
    <defName>WoodlandFlakVest</defName>
    <label>flak vest (woodland)</label>
    <description>A vest with armor plates inserted on the chest and back. While quite effective at deflecting gunshots and stabbing attacks against the chest, it does nothing to protect the limbs. This vest has a woodland camo design.</description>
    <techLevel>Industrial</techLevel>
    <recipeMaker>
      <researchPrerequisite>FlakArmor</researchPrerequisite>
      <unfinishedThingDef>UnfinishedTechArmor</unfinishedThingDef>
      <skillRequirements>
        <Crafting>5</Crafting>
      </skillRequirements>
    </recipeMaker>
    <graphicData>
      <texPath>Things/Item/FlakArm/Woodland/Apparel/WoodlandFlakVest</texPath>
      <graphicClass>Graphic_Single</graphicClass>
      <drawSize>0.90</drawSize>
    </graphicData>
    <statBases>
      <WorkToMake>9200</WorkToMake>
      <MaxHitPoints>200</MaxHitPoints>
      <Mass>4</Mass>
      <Flammability>0.6</Flammability>
      <ArmorRating_Sharp>1.00</ArmorRating_Sharp>
      <ArmorRating_Blunt>0.36</ArmorRating_Blunt>
      <ArmorRating_Heat>0.27</ArmorRating_Heat>
      <Insulation_Cold>1</Insulation_Cold>
      <EquipDelay>5</EquipDelay>
    </statBases>
    <equippedStatOffsets>
      <MoveSpeed>-0.12</MoveSpeed>
    </equippedStatOffsets>
    <thingCategories Inherit="false">
      <li>CASApparel</li>
    </thingCategories>
    <costList>
      <Cloth>30</Cloth>
      <Steel>60</Steel>
      <ComponentIndustrial>1</ComponentIndustrial>
    </costList>
    <apparel>
      <bodyPartGroups>
        <li>Torso</li>
        <li>Neck</li>
        <li>Shoulders</li>
      </bodyPartGroups>
      <wornGraphicPath>Things/Item/FlakArm/Woodland/Body/Vest/WoodlandFlakVest</wornGraphicPath>
      <layers>
        <li>Middle</li>
      </layers>
      <tags>
        <li>PassiveCamo_Woodland_High</li>
        <li>IndustrialMilitaryBasic</li>
      </tags>
      <defaultOutfitTags>
        <li>Soldier</li>
      </defaultOutfitTags>
    </apparel>
  </ThingDef>
